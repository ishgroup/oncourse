<t:container
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">
	<span class="${feeClass}">
		<t:output value="courseClass.feeIncGst" format="moneyFormat"/>
	</span>
	<span class="${feeDiscountedClass})">
		<t:output value="enrolment.discountedIncTaxAmount" format="moneyFormat"/>
	</span>
	<span id="d${index}" style="display:none;">
		<t:output value="enrolment.totalDiscountIncTax" format="numberFormat"/>
	</span>
</t:container>	