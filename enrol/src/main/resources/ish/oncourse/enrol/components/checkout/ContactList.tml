<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
		>
	<t:zone t:id="contactsZone" t:update="show">
	<t:loop source="contacts" value="contact">
		<span t:type="checkout/contactitem" contact="contact"
			  purchaseController="purchaseController"/>
	</t:loop>
	</t:zone>
</t:container>