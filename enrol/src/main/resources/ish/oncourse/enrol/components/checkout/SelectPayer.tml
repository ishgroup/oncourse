<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<t:zone t:id="contactsZone" t:update="show">
		<span class="payer" style="padding-left:5px;">Payer</span>
		<span class="valid">
			<t:select t:id="currentPayer" value="selectedContact" model="payersModel" encoder="payersEncoder" blankOption="NEVER"
				zone="contactsZone"/>
		</span>
	</t:zone>
</t:container>