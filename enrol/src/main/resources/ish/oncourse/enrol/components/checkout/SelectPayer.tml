<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<div class="drop-down">
		<span class="btn-primary">Payer <b class="caret"></b></span>
		<ul class="drop-down-content">
			<t:loop source="purchaseController.model.contacts" value="contact" index="index">
				<li><a href="javascript: void(0);" rel="payer_${index}" class="${itemClass}"><font>${contact.fullName}</font></a></li>
			</t:loop>
		</ul>
	</div>
</t:container>