<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
		>
	<t:loop source="enrolments" value="enrolment" index="index">
		<span t:type="checkout/enrolmentitem"
			  enrolment="enrolment"
			  enrolmentIndex="index"
			  contactIndex="contactIndex"
			  checked="checked"
			  delegate="enrolmentItemDelegate" blockToRefresh="blockToRefresh"/>
	</t:loop>
</t:container>