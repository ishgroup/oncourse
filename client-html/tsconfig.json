{
  "compilerOptions": {
    "rootDirs": [
      "src/js",
      "src/images",
      "src/fonts",
      "build/generated-sources/aql-parser",
      "build/generated-sources/aql-model",
      "build/generated-sources/swagger-js"
    ],
    "baseUrl": ".",
    "paths": {
      "@aql/*": ["build/generated-sources/aql-parser/*", "build/generated-sources/aql-model/*"],
      "@api/model": ["build/generated-sources/swagger-js/api.ts"],
      "@api/configuration": ["build/generated-sources/swagger-js/configuration.ts"]
    },
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "module": "esnext",
    "skipLibCheck": true,
    "target": "es2015",
    "jsx": "react",
    "lib": [
      "dom",
      "esnext",
      "es5",
      "es6",
      "es7"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "./node_modules*",
    "./build/resources*",
    "./src/api-docs*"
  ]
}
