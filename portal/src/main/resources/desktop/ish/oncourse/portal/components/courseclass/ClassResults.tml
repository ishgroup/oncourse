<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
<t:if test="visible">
<div id="class-results" class="tab-pane">
    <ul class="list-unstyled result-content">
        <li>
            <t:if test="qualification">
                <p>Assessments are made against units of competency from the ${qualification.title} <a href="http://training.gov.au/Training/Details/${qualification.nationalCode}" target="_blank"> ${qualification.nationalCode}</a>.</p>
            </t:if>
            <t:unless test="qualification">
                <p>This course does not have a qualification</p>
            </t:unless>

            <t:if test="hasModules">
            <ul class="list-unstyled result-list">

                <t:loop source="modules" value="module">
                     <li>
                         <span>${module.title} (<a href="http://training.gov.au/Training/Details/${module.nationalCode}">${module.nationalCode}</a>)</span>
                         <strong class="${getOutComeClass(module.id)} tooltip-info" title="" data-original-title="${getOutComeDisplayName(module.id)}">${getOutComeResult(module.id)}</strong>
                     </li>
                </t:loop>
            </ul>
            </t:if>

        </li>
    </ul>
</div>
</t:if>
</t:container>
