<html t:type="pagestructure" title="literal:Profile"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:m_surveys">

	<div class="survey-result-title">
		<span class="survery-title">${courseClass.course.name}</span>
		<div class="clear"></div>
	</div>
	<ul class="raging-info">
		<li>
			<span>
				<i class="icon-user icon-white"></i>
				<t:loop  source="tutorRoles" value="tutorRole" formState="none">
					${tutorRoleName}
				</t:loop>
			</span>
			<font>
				<span t:type="rating" t:id="ratingTutor" delegate="tutorRatingDelegate" value="survey.tutorScore"/>
			</font>
		</li>
		<li>
			<span>
				<i class="icon-file icon-white"></i>${courseName}
			</span>
			<font>
				<span t:type="rating" t:id="ratingCourse" delegate="courseRatingDelegate" value="survey.courseScore"/>
			</font>
		</li>
		<li>
			<span>
				<i class="icon-home icon-white"></i>${room}
			</span>
			<font>
				<span t:type="rating" t:id="ratingVenue" delegate="venueRatingDelegate" value="survey.venueScore"/>
			</font>
		</li>
	</ul>
	
	<div class="mobile-survey">
		<t:form t:id="approvalForm">
			<t:textarea t:id="whyDeclined" placeholder="Comments" row="7"></t:textarea>
			<div class="clear"></div>
			<t:linksubmit t:id="accept" class="btn">Submit</t:linksubmit>
		</t:form>
	</div>

</html>