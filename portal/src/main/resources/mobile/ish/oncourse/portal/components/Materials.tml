<html t:type="pagestructure" title="literal:Mailing Lists"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter" activeMenu="literal:m_mailinglists">
	
		<h4 class="heading">Materials</h4>
		<ul class="material-list">
			<t:loop source="materials" value="material">
				<li><a href="${material.contextPath}">${material.name} - ${material.size}kb</a><i class="icon-chevron-right pull-right"></i></li>
			</t:loop>
		</ul>
	
</html>
