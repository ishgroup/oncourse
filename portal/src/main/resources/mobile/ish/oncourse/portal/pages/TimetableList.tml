<html t:type="pagestructure" title="literal:Timetable list" bodyClass="literal:login-wrapper"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	
	<h1>Timetable</h1>
	
	<div class="btn-tabs">
		<div class="btn-group btn-tabs">
			<button id="timetableMonth" page="prop:monthPageName" class="btn">Month</button>
			<button id="timetableList" page="prop:listPageName" class="btn btn-active disabled">List</button>
		</div>
	</div>
	<div class="clear"></div>
	
	<div id="list" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
		<t:if test="hassessions">
			<t:loop source="sessionsByDate" value="currentBucket">
				<t:loop source="currentBucket.value" value="session">
					<p>${time}.</p>
					<strong><a href="#">${name}</a></strong>
				</t:loop>
				
			</t:loop>
			
			<p:else>
				You have no currently scheduled classes.
			</p:else>
		</t:if>
	</div>
