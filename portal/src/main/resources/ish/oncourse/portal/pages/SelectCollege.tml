<html t:type="pagestructure" title="literal:Select College"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	
		<h1>Select College</h1>
		<t:form t:id="collegeForm">
			<t:errors banner="literal:Please correct the following mistakes:" class="literal:validation"/>
			<t:radiogroup t:value="selectedUser">
				<t:loop source="users" value="user">
					<input t:type="radio" value="user"/>${collegeName}<br/>
				</t:loop>
			</t:radiogroup>
			<br/>
			<t:submit value="Select"/>
		</t:form>
</html>