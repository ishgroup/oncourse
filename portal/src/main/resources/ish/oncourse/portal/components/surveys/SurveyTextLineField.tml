<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
    <li>
        <label title="${field.description}">${field.name}<t:if test="${field.mandatory}"> *</t:if></label>
        <input name="customField"  type="text"  data-default="${defaultValue}" data-type="${dataType}"  value="${getValue()}" data-key="${field.property}"/>
    </li>
</t:container>