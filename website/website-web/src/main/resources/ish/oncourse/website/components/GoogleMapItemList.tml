<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">

	<t:if t:test="hasMapItemList">
		<!--
			TODO <t:if t:test="$hasTag"><t:output object="$tag"
			displayKey="detail" escapeHTML="$false" /></t:if>
		-->
		<div id="focus-map" class="collapse">
			<span t:type="GoogleDirections" latitude="mapPositionLatitude"
				longitude="mapPositionLongitude" hideDirections="true"
				showNearMarker="hasGlobalPosition" showMapItems="true"
				showLocationMap="false" near="searchingNear" />
		</div>
	</t:if>

</t:container>