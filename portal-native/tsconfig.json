{
  "compilerOptions": {
    "rootDirs": [
      "src/js",
      "src/assets",
      "build/generated-sources"
    ],
    "baseUrl": ".",
    "paths": {
      "@api/model": ["build/generated-sources/api.ts"],
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "strict": true
  },
  "extends": "expo/tsconfig.base"
}
