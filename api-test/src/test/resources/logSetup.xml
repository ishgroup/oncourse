<?xml version="1.0" encoding="UTF-8"?>


<Configuration status="INFO">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5p %c:%L - %m %X%x%n%ex"/>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="ERROR">
			<AppenderRef ref="STDOUT"/>
		</Root>
	</Loggers>

</Configuration>
