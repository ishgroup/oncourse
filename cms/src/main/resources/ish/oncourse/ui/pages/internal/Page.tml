<html t:type="ui/internal/pagestructure" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      bodyId="${bodyId}" bodyClass="${bodyClass}" webNodeType="webNodeType" webNode="editNode">
