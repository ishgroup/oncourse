{
  "name": "billing",
  "version": "0.1.0",
  "private": true,
  "main": "webpack.config.js",
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-utils": "latest",
    "@ckeditor/ckeditor5-theme-lark": "latest",
    "awesome-typescript-loader": "5.0.0-1",
    "file-loader": "5.*",
    "source-map-loader": "0.2.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "formik": "^2.2.6",
    "html-webpack-plugin": "^4.5.1",
    "node-sass": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "dependencies:outdated": "npm outdated",
    "clean": "rm -rf ./build",
    "build:prod": "webpack -p --progress --env.NODE_ENV production",
    "build:prod:watch": "webpack --watch --progress -p --env.NODE_ENV production",
    "build:dev": "webpack --progress",
    "build:dev:watch": "webpack --debug --watch --env.SOURCE_MAP eval-source-map",
    "test": "jest --ci  --testResultsProcessor=jest-junit",
    "start": "webpack-dev-server --history-api-fallback=true --progress --open --config webpack.dev.config.js --env.entry=./src/js/app.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
