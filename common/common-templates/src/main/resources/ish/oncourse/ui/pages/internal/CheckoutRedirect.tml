<html>
<head>
    <script>
        const query = new URLSearchParams(window.location.search);
        const sessionId = query.get("sessionId");
        const status = query.get("paymentStatus");
        if (sessionId ) {
            if (status) {
                window.parent.postMessage(
                    {
                        payment: {
                            sessionId,
                            status
                        }
                    },
                    "*"
                );
            }
        }
    </script>
</head>
<body>
</body>
</html>