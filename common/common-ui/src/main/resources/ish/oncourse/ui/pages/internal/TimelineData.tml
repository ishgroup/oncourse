<?xml version="1.0" encoding="UTF-8" ?>
<data xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	<t:loop source="records" value="record">
		<event start="${startDate}" end="${endDate}" title="${recordTitle}" link="/class/${record.courseClass.uniqueIdentifier}"
			icon="/s/img/orange.png" color="orange"> ${eventContent} </event>
	</t:loop>
</data>
