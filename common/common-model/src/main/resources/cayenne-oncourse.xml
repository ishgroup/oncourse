<?xml version="1.0" encoding="utf-8"?>
<domain project-version="6">
	<property name="cayenne.DataDomain.dataContextFactory" value="ish.oncourse.model.access.ISHDataContextFactory"/>

	<map name="oncourse"/>
	<map name="oncourseBinary"/>

	<node name="oncourse"
		 factory="org.apache.cayenne.configuration.server.JNDIDataSourceFactory"
		 parameters="jdbc/oncourse"
		>
		<map-ref name="oncourse"/>
	</node>
	<node name="oncourseBinary"
		 factory="org.apache.cayenne.configuration.server.JNDIDataSourceFactory"
		 parameters="jdbc/oncourse_binary"
		>
		<map-ref name="oncourseBinary"/>
	</node>
</domain>
