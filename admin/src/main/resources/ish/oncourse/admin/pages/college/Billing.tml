<html t:type="pagestructure" title="literal:College Billing"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">

<span t:type="editTabs" college="college" selected="literal:billing" />

<h1>${college.name}</h1>

<t:form t:id="billingForm">
	<table>
		<tr>
			<td>PaymentExpress user</td>
			<td><t:textfield t:id="paymentUser" value="paymentExpUser" />
			</td>
		</tr>
		<tr>
			<td>PaymentExpress password</td>
			<td><t:textfield t:id="paymentPass" value="paymentExpPass" />
			</td>
		</tr>
	</table>

	<h2>License Fees</h2>

	<table>
		<thead>
			<tr>
				<td>Key Code</td>
				<td>Plan Name</td>
				<td>Billing Month</td>
				<td>Valid Until</td>
				<td>Free Transactions</td>
				<td>Fee</td>
			</tr>
		</thead>
		<tbody>
			<t:loop source="licenseInfo.keySet()" value="currentLicenseInfoKey">
				<tr>
					<td>${currentLicenseInfoKey}</td>
					<t:loop source="currentLicenseInfo.keySet()" value="infoKey">
						<td><t:textfield t:id="infoKey" value="infoValue"/></td>
					</t:loop>
				</tr>
			</t:loop>
		</tbody>
	</table>
	
	<t:submit t:id="save" value="Save"/>
</t:form>

</html>