<html t:type="pagestructure" title="literal:Willow Admin Home"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">

 	<table class="table table-striped table-condensed table-bordered table-hover">
 		<thead>
			<tr class="row">
				<th class="span1">ID</th>
				<th class="span8">Name</th>
				<th class="span2">State</th>
				<th class="span6">Last Remote Authentication</th>
				<th class="span4">Angel Version</th>
				<th class="span2">Site</th>
			</tr>
 		</thead>
 		<tbody>
 			<t:loop source="colleges" value="college">
 				<tr class="row">
 					<td>${college.id}</td>
 					<td><a t:type="pagelink" t:page="college/overview" t:context="college.id" href="#">${college.name}</a></td>
					<td>${state}</td>
 					<td><code>${lastRemoteAuthentication}</code></td>
 					<td>${college.angelVersion}</td>
					<td>
						<t:loop source="college.webSites" value="site">
							<a target="_blank" href="http://${site.siteKey}.oncourse.cc">${site.siteKey}<t:if test="commaSeparate">, </t:if></a>
						</t:loop>
					</td>
 				</tr>
 			</t:loop>
 		</tbody>
 	</table>

</html>
