<html t:type="pagestructure" title="literal:Add College"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	
		<table class="table table-striped table-condensed">
			<tr>
				<td>Services Key</td>
				<td><input id="serviceKey" type="text"/></td>
			</tr>
			<tr>
				<td><button id="find" class="btn btn-primary">Find</button>
					<t:eventlink t:id="findCollegeEvent" style="{display:none}" class="findServiceKeyLink"/>
				</td>
				</tr>
		</table>
	
	<div id="results">
	</div>
	
	<div id="newCollege">
		<t:form t:id="newCollegeForm">
			<input id="collegeId" t:type="hidden" property="collegeId" value="collegeId"/>
			<table class="table table-striped table-condensed">
				<tr>
					<td>Billing Code</td>
					<td><t:textfield t:id="billingCode" t:validate="required" value="billingCode"/></td>
				</tr>
				<tr>
					<td>College Key</td>
					<td><t:textfield t:id="collegeKey" t:validate="required" maxlength="32" value="collegeKey"/></td>
				</tr>
			</table>
			<div id="newCollegeValidationResult">
			</div>
		</t:form>
		<button id="addNewCollege" class="btn btn-primary">Next</button>
		<t:eventlink t:id="addBillingCodeAndCollegeKeyEvent" style="{display:none}" class="addBillingCodeAndCollegeKeyLink"/>
	</div>
</html>