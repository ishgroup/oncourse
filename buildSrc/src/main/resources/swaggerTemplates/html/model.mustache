{{#models}}
    {{#model}}

        <h2>{{classname}}</h2>
        {{#title}}<h3>{{title}}</h3>{{/title}}
        <p class='model-description'>{{description}}</p>
        {{#vars}}
            <ul class="parameter">
                <li class="param-required-{{required}}">{{name}} (<span class="param-type">{{datatype}}</span>)
                    <div class="param-desc">{{description}}</div>
                    {{#isEnum}}
                        <dl class="param-enum">
                        <dt>Enum:
                            {{#_enum}}
                                <dd>{{this}}</dd>
                            {{/_enum}}
                            </dt>
                        </dl>
                    {{/isEnum}}
                </li>
            </ul>
        {{/vars}}
    {{/model}}
{{/models}}



