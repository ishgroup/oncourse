<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	<ul class="courseList">	
		<t:loop source="courses" value="course">
			<li data-courseCode="${course.code}">
				<a href="/course/${course.code}">${course.name}</a>
				<p><t:outputraw value="course.detail"/></p>
			</li>
		</t:loop>	
	</ul> 
</t:container>