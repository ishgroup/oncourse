  "jest": {
    "globals": {
      "_APP_VERSION": "DEV"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/jest/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "jsdom"
  },
  "jest-junit": {
    "suiteName": "checkout tests",
    "output": "./build/test-results/test/results.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "resolutions": {
    "webpack-bugsnag-plugins/bugsnag-build-reporter/meow/trim-newlines": "^3.0.1",
    "webpack-dev-server/chokidar/glob-parent": "^5.1.2"
