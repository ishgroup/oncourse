{
  "name": "checkout",
  "version": "1.0.0",
  "description": "Checkout application",
  "main": "webpack.config.js",
  "scripts": {
    "dependencies:update": "npm prune && npm install",
    "dependencies:outdated": "npm outdated",
    "clean": "rm -rf ./build ./out",
    "build:prod": "npm run clean && yarn install && webpack --progress --env NODE_ENV=production",
    "build:prod:watch": "webpack --watch --progress -p --env.NODE_ENV production",
    "build:dev": "npm run clean && webpack --progress --config webpack.dev.config.js",
    "build:dev:watch": "webpack --debug --watch --env SOURCE_MAP=eval-source-map",
    "test": "jest --ci  --testResultsProcessor=jest-junit",
    "test:watch": "karma start ./karma.conf.js --auto-watch --no-single-run",
    "start": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/js/app.tsx  --content-base ./dev-server",
    "start:dev": "webpack serve --progress --open --config webpack.dev.config.js --content-base ./dev-server",
    "start:dev:CheckoutApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/CheckoutApp.tsx  --content-base ./dev-server",
    "start:dev:SummaryApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/summary/SummaryApp.tsx  --content-base ./dev-server",
    "start:dev:ContactEditFormApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/contact-edit/ContactEditFormApp.tsx --content-base ./dev-server",
    "start:dev:ReactSelectFieldApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/components/form-new/ReactSelectFieldApp.tsx --content-base ./dev-server",
    "start:dev:AllComponentsInFormApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/components/form-new/AllComponentsInFormApp.tsx --content-base ./dev-server",
    "start:dev:AllComponentsApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/components/form-new/AllComponentsApp.tsx --content-base ./dev-server",
    "start:dev:PaymentApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/payment/PaymentApp.tsx --content-base ./dev-server",
    "start:dev:ConcessionApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/concession/ConcessionApp.tsx --content-base ./dev-server",
    "start:dev:PaymentInProgressApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/result/PaymentInProgressApp.tsx --content-base ./dev-server",
    "start:dev:PaymentAllComponentsApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/result/PaymentAllComponentsApp.tsx --content-base ./dev-server",
    "start:dev:ApplicationCompApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/summary/ApplicationCompApp.tsx --content-base ./dev-server",
    "start:dev:MembershipCompApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/summary/MembershipCompApp.tsx --content-base ./dev-server",
    "start:dev:ArticleCompApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/checkout/summary/ArticleCompApp.tsx --content-base ./dev-server",
    "start:dev:FeesComponentApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/web/components/fees/FeesComponentApp.tsx --content-base ./dev-server/web/components/fees",
    "start:dev:FeesRangeComponentApp": "webpack serve --progress --open --config webpack.dev.config.js --entry ./src/dev/web/components/fees/FeesRangeComponentApp.tsx --content-base ./dev-server/web/components/fees",
    "deploy": "cp -vf ./build/dist/dynamic.* /Users/akoiro/_data/_ish/tomonCourse/s/ak-react/js"
  },
