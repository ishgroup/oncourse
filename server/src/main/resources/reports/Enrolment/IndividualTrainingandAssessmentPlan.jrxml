						<font fontName="Open Sans" size="11"/>
					</textElement>
					<text><![CDATA[OBLIGATIONS AND UNDERTAKINGS]]></text>
				</staticText>
				<staticText>
					<reportElement key="staticText-1" style="Label" x="20" y="61" width="458" height="48" forecolor="#000000" uuid="4f286522-a797-42e1-bc3d-7fef104b2d99">
