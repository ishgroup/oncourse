{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": false,
    "object-curly-spacing": [
      true,
      "never"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "max-line-length": [
      true,
      120
    ],
    "quotemark": false,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "ter-arrow-parens": false
  }
}
