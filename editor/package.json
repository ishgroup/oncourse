  "jest": {
    "globals": {
      "_APP_VERSION": "DEV"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/jest/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src/js",
      "src/test"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "jsdom"
  },
  "jest-junit": {
    "suiteName": "cms tests",
    "output": "./build/test-results/test/results.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "resolutions": {
    "@ckeditor/ckeditor5-dev-utils/**/is-svg": "^4.2.2",
    "@ckeditor/ckeditor5-markdown-gfm/**/marked": "^2.0.0",
    "webpack-dev-server/**/ws": "^7.4.6"
  }
}
