{
  "name": "editor",
  "version": "1.0.0",
  "description": "cms pure java script application",
  "main": "webpack.config.js",
  "scripts": {
    "dependencies:outdated": "npm outdated",
    "clean": "rm -rf ./build",
    "build:prod": "webpack -p --progress --node-env production",
    "build:prod:watch": "webpack --watch --progress -p --node-env production",
    "build:dev": "webpack --progress",
    "build:dev:watch": "webpack --debug --watch --env.SOURCE_MAP eval-source-map",
    "build:mock": "webpack --progress --node-env mock",
    "build:mock:watch": "webpack --debug --watch --env.SOURCE_MAP eval-source-map --node-env mock",
    "test": "jest --ci  --testResultsProcessor=jest-junit --passWithNoTests",
    "start": "webpack serve --history-api-fallback --progress --open --config webpack.dev.config.js --entry ./src/js/app.tsx --content-base ./src/dev",
    "start:prod": "webpack serve --history-api-fallback --progress --open --config webpack.dev.config.js --entry ./src/js/app.tsx --content-base ./src/dev",
    "start:mock": "webpack serve --history-api-fallback --progress --open --config webpack.dev.config.js --entry ./src/dev/app.tsx --content-base ./src/dev"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-utils": "^24.4.2",
