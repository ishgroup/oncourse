<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:jaxws="http://cxf.apache.org/jaxws" xmlns:cxf="http://cxf.apache.org/core"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
			http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd
			http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<context:annotation-config />

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<import resource="classpath:application-context-usi-2015.xml" />

	<bean id="gzipFeature" class="org.apache.cxf.transport.common.gzip.GZIPFeature" />
	<bean id="loggingFeature" class="org.apache.cxf.feature.LoggingFeature" />

	<bean id="v4_referencePortTypeBean" class="ish.oncourse.webservices.soap.v4.ReferencePortTypeImpl" />

	<bean id="v5_referencePortTypeBean" class="ish.oncourse.webservices.soap.v5.ReferencePortTypeImpl" />

	<bean id="v6_referencePortTypeBean" class="ish.oncourse.webservices.soap.v6.ReferencePortTypeImpl" />
	
	<bean id="v4_securityCodeInterceptor" class="ish.oncourse.webservices.soap.v4.interceptors.SecurityCodeInterceptor" />

	<bean id="v4_communicationKeyInterceptor" class="ish.oncourse.webservices.soap.v4.interceptors.CommunicationKeyInterceptor" />
	
	<bean id="v4_collegeRequestInterceptor" class="ish.oncourse.webservices.soap.v4.interceptors.CollegeRequestInterceptor" />

	<bean id="v10_paymentPortType" class="ish.oncourse.webservices.soap.v10.PaymentPortTypeImpl" />

	<bean id="v10_replicationPortType" class="ish.oncourse.webservices.soap.v10.ReplicationPortTypeImpl" />

	<bean id="v11_paymentPortType" class="ish.oncourse.webservices.soap.v11.PaymentPortTypeImpl" />

	<bean id="v11_replicationPortType" class="ish.oncourse.webservices.soap.v11.ReplicationPortTypeImpl" />

	<bean id="v12_paymentPortType" class="ish.oncourse.webservices.soap.v12.PaymentPortTypeImpl" />

	<bean id="v12_replicationPortType" class="ish.oncourse.webservices.soap.v12.ReplicationPortTypeImpl" />

	<bean id="v13_paymentPortType" class="ish.oncourse.webservices.soap.v13.PaymentPortTypeImpl" />

	<bean id="v13_replicationPortType" class="ish.oncourse.webservices.soap.v13.ReplicationPortTypeImpl" />

	<bean id="v14_paymentPortType" class="ish.oncourse.webservices.soap.v14.PaymentPortTypeImpl" />

	<bean id="v14_replicationPortType" class="ish.oncourse.webservices.soap.v14.ReplicationPortTypeImpl" />

	<bean id="v15_paymentPortType" class="ish.oncourse.webservices.soap.v15.PaymentPortTypeImpl" />

	<bean id="v15_replicationPortType" class="ish.oncourse.webservices.soap.v15.ReplicationPortTypeImpl" />
	
	<cxf:bus>
		<cxf:features>
			<ref bean="gzipFeature" />
			<ref bean="loggingFeature" />
		</cxf:features>
	</cxf:bus>

	<jaxws:endpoint id="v4_reference" implementor="#v4_referencePortTypeBean"
		address="/v4/reference">
		<jaxws:inInterceptors>
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
    </jaxws:endpoint>

	<jaxws:endpoint id="v5_reference" implementor="#v5_referencePortTypeBean"
					address="/v5/reference">
		<jaxws:inInterceptors>
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v6_reference" implementor="#v6_referencePortTypeBean"
					address="/v6/reference">
		<jaxws:inInterceptors>
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>
	
	<jaxws:endpoint id="v10_replication" implementor="#v10_replicationPortType"
					address="/v10/replication">
		<jaxws:inInterceptors>
			<ref bean="v4_communicationKeyInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v10_payment" implementor="#v10_paymentPortType"
					address="/v10/payment">
		<jaxws:inInterceptors>
			<ref bean="v4_securityCodeInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v11_replication" implementor="#v11_replicationPortType"
					address="/v11/replication">
		<jaxws:inInterceptors>
			<ref bean="v4_communicationKeyInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v11_payment" implementor="#v11_paymentPortType"
					address="/v11/payment">
		<jaxws:inInterceptors>
			<ref bean="v4_securityCodeInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v12_replication" implementor="#v12_replicationPortType"
					address="/v12/replication">
		<jaxws:inInterceptors>
			<ref bean="v4_communicationKeyInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v12_payment" implementor="#v12_paymentPortType"
					address="/v12/payment">
		<jaxws:inInterceptors>
			<ref bean="v4_securityCodeInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v13_replication" implementor="#v13_replicationPortType"
					address="/v13/replication">
		<jaxws:inInterceptors>
			<ref bean="v4_communicationKeyInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v13_payment" implementor="#v13_paymentPortType"
					address="/v13/payment">
		<jaxws:inInterceptors>
			<ref bean="v4_securityCodeInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v14_replication" implementor="#v14_replicationPortType"
					address="/v14/replication">
		<jaxws:inInterceptors>
			<ref bean="v4_communicationKeyInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v14_payment" implementor="#v14_paymentPortType"
					address="/v14/payment">
		<jaxws:inInterceptors>
			<ref bean="v4_securityCodeInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v15_replication" implementor="#v15_replicationPortType"
					address="/v15/replication">
		<jaxws:inInterceptors>
			<ref bean="v4_communicationKeyInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>

	<jaxws:endpoint id="v15_payment" implementor="#v15_paymentPortType"
					address="/v15/payment">
		<jaxws:inInterceptors>
			<ref bean="v4_securityCodeInterceptor" />
			<ref bean="v4_collegeRequestInterceptor" />
		</jaxws:inInterceptors>
	</jaxws:endpoint>
	
</beans>