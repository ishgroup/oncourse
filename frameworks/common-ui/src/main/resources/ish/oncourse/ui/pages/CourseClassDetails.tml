<html t:type="ui/pagewrapper" title="${title}"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	<h2 class="clearfix">
		<span>
			<a href="/course/${courseClass.course.code}">${courseClass.course.name}</a>
		</span>
	</h2>
	<p class="print-page noprint" onclick="window.print();">
		Print this page
		<input type="button" value="Print" />
	</p>
	<div id="class_detail" class="new_course_item">
		<span t:type="ui/courseclassitem" courseClass="courseClass" />
		<t:outputraw value="${courseDetail}" />
		<t:outputraw value="${courseClassDetail}" />
		<t:if t:test="${courseClass.hasRoom}">
			<h3>Location</h3>
			<spap t:type="ui/RoomDetail" room="courseClass.room"
				collapseLocationMap="true" />
		</t:if>
		<div class="blockdetail">
			<t:if t:test="${showInlineTimetable}">
				<t:if t:test="courseClass.hasAnyTimelineableSessions">
					<h2>Sessions</h2>
					<span t:type="ui/TimetableEvents" displayedObjects="sortedTimelineableSessions"
						headerLabels="timetableLabels" cssTableClass="cssTableClass"
						cssEvenRowClass="cssEvenRowClass" cssOddRowClass="cssOddRowClass"
						dateFormat="dateFormat" timeFormat="timeFormat" />
				</t:if>
			</t:if>
		</div>
	</div>
</html>