<ul xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	
	<t:loop source="mainMenu.navigableChildMenus" value="currentMenu">
		<t:delegate to="menuItem"/>
	</t:loop>
	
    <t:block>
        <div t:id="menuItem">
            <t:delegate to="menuItem"/>
        </div>
    </t:block>

</ul>