{
  "name": "ish",
  "version": "0.0.0",
  "description": "ish project",
  "main": "webpack.config.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:prod": "npm run clean && webpack -p --env.NODE_ENV production --env.API_ROOT https://ish.com.au/api/v1",
    "build:prod:watch": "webpack --watch -p --env.NODE_ENV production --env.API_ROOT https://ish.com.au/api/v1",
    "build:dev": "npm run clean && webpack --env.SOURCE_MAP eval-source-map",
    "build:dev:watch": "webpack --debug --watch --env.SOURCE_MAP eval-source-map",
    "test": "karma start ./karma.conf.js",
    "test:watch": "karma start ./karma.conf.js --auto-watch --no-single-run",
    "start": "node ./dev-server/generate.js && webpack-dev-server --env.SOURCE_MAP eval-source-map"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.41",
    "@types/jquery": "^2.0.40",
    "@types/lodash": "^4.14.52",
    "@types/react-redux": "^4.4.36",
    "@types/webpack": "^2.2.6",
    "@types/webpack-env": "^1.13.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "css-loader": "0.26.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "jsdoc": "3.4.3",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "ts-loader": "^2.0.0",
    "typedoc": "^0.5.5",
    "typedoc-webpack-plugin": "^1.1.3",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "classnames": "2.2.5",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-redux": "5.0.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  }
}
