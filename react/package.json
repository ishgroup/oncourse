{
  "name": "ish",
  "version": "0.0.1",
  "description": "ish project",
  "main": "webpack.config.js",
  "scripts": {
    "packages:refresh": "npm prune && npm install",
    "clean": "rm -rf ./dist",
    "build:prod": "npm run clean && webpack -p --env.NODE_ENV production --env.API_ROOT https://ish.com.au",
    "build:prod:watch": "webpack --watch -p --env.NODE_ENV production --env.API_ROOT https://ish.com.au",
    "build:dev": "npm run clean && webpack --env.SOURCE_MAP eval-source-map",
    "build:dev:watch": "webpack --debug --watch --env.SOURCE_MAP eval-source-map",
    "test": "karma start ./karma.conf.js",
    "test:watch": "karma start ./karma.conf.js --auto-watch --no-single-run",
    "start": "node ./dev-server/generate.js && webpack-dev-server --env.SOURCE_MAP eval-source-map"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "@types/jquery": "2.0.40",
    "@types/lodash": "4.14.58",
    "@types/react-redux": "4.4.38",
    "@types/redux-form": "6.3.6",
    "@types/webpack": "2.2.6",
    "@types/webpack-env": "1.13.0",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jasmine-core": "2.5.2",
    "jsdoc": "3.4.3",
    "karma": "1.4.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.14",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "typedoc": "0.5.9",
    "typedoc-webpack-plugin": "1.1.3",
    "typescript": "2.2.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "axios": "0.15.3",
    "babel-polyfill": "6.20.0",
    "classnames": "2.2.5",
    "jquery": "3.1.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "normalizr": "3.2.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "redux": "3.6.0",
    "redux-form": "6.6.0",
    "redux-logger": "3.0.0",
    "redux-observable": "0.14.1",
    "rodal": "1.4.0",
    "rxjs": "5.2.0"
  }
}
