{
  "name": "ish",
  "version": "0.0.1",
  "description": "ish project",
  "main": "webpack.config.js",
  "scripts": {
    "dependencies:update": "npm prune && npm install",
    "dependencies:outdated": "npm outdated",
    "clean": "rm -rf ./build",
    "build:prod": " npm run dependencies:update && npm run clean && webpack --progress -p --env.NODE_ENV production --env.API_ROOT https://ish.com.au",
    "build:prod:watch": "webpack --watch --progress -p --env.NODE_ENV production --env.API_ROOT https://ish.com.au",
    "build:dev": "npm run clean && webpack",
    "build:dev:watch": "webpack --debug --watch --env.SOURCE_MAP eval-source-map",
    "test": "jest",
    "test:watch": "karma start ./karma.conf.js --auto-watch --no-single-run",
    "start": "webpack-dev-server --progress --open --config webpack.dev.config.js --env.entry=./src/js/app.ts  --content-base ./dev-server",
    "start:dev": "webpack-dev-server --progress --open --config webpack.dev.config.js --content-base ./dev-server"
  },
  "devDependencies": {
    "@types/jquery": "2.0.40",
    "@types/lodash": "4.14.58",
    "@types/react-autosuggest": "8.0.1",
    "@types/react-redux": "4.4.38",
    "@types/redux-form": "6.6.2",
    "@types/webpack": "2.2.6",
    "@types/webpack-env": "1.13.0",
    "@types/jest": "19.2.2",
    "css-loader": "0.28.0",
    "sass-loader": "6.0.5",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jsdoc": "3.4.3",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.14",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "source-map-loader": "0.2.1",
    "typedoc": "0.5.10",
    "typedoc-webpack-plugin": "1.1.4",
    "typescript": "2.2.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",

    "jest": "19.0.2",
    "ts-jest": "19.0.14",
    "identity-obj-proxy": "3.0.0",
    "tslint": "5.2.0",
    "redux-form-website-template": "0.0.41"
  },
  "dependencies": {
    "axios": "0.15.3",
    "babel-polyfill": "6.20.0",
    "classnames": "2.2.5",
    "jquery": "3.1.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "normalizr": "3.2.2",
    "react": "15.4.2",
    "react-autosuggest": "9.0.0",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-text-mask": "5.0.0",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "redux-logger": "3.0.0",
    "redux-observable": "0.14.1",
    "redux-persist": "4.8.0",
    "localforage": "1.5.0",

    "react-select": "1.0.0-rc.4",
    "rodal": "1.4.0",
    "rxjs": "5.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/jest/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "jsdom"
  }
}
